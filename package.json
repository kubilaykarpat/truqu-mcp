{
    "name": "truqu-mcp",
    "version": "0.1.4",
    "description": "A MCP server for interacting with Truqu goal and feedback data",
    "main": "dist/index.js",
    "bin": {
        "truqu-mcp": "dist/index.js"
    },
    "type": "module",
    "files": [
        "dist/**/*",
        "README.md",
        "package.json"
    ],
    "scripts": {
        "build": "tsc",
        "dev": "tsx src/index.ts",
        "start": "node dist/index.js",
        "watch": "tsx watch src/index.ts",
        "clean": "rm -rf dist",
        "prepare": "npm run build"
    },
    "keywords": [
        "mcp",
        "model-context-protocol",
        "typescript"
    ],
    "author": {
        "name": "Kubilay Karpat",
        "email": "karpatkubilay@gmail.com",
        "url": "https://github.com/kubilaykarpat/"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:kubilaykarpat/truqu-mcp.git"
    },
    "license": "MIT",
    "dependencies": {
        "@modelcontextprotocol/sdk": "^0.5.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "tsx": "^4.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
